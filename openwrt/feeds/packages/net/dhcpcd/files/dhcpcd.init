# START=85
# STOP=85
USE_PROCD=1

start_service()
{
	procd_open_instance
	#procd_set_param command /sbin/dhcpcd -B
	procd_set_param command /sbin/dhcpcd -B wlan0 -y 30
	procd_set_param respawn 3600 2 -1
	procd_close_instance
}

